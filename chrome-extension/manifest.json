{
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "현재 보고 있는 기사 찍기"
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://localhost:8000/*"
  ],
