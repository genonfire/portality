  //   "page": "options.html",
  //   "chrome_style": false
  // },
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "manifest_version": 2,
  "name": "좌표 찍기",
  "description": "기레기 감시하기 위해 기사의 좌표를 찍습니다.",
